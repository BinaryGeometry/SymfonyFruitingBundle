{
    "name": "binary-geometry/fruiting-bundle",
    "description": "This bundle provides fruit for the family",
    "keywords": ["annotations","controllers"],
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Andrew MacKay",
            "email": "andrew.mackay@binarygeometry.co.uk",
            "homepage": "http://binarygeometry.co.uk",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.2.5",
        "doctrine/annotations": "^1.0|^2.0",
        "symfony/config": "^4.4|^5.0|^6.0",
        "symfony/dependency-injection": "^4.4|^5.0|^6.0",
        "symfony/framework-bundle": "^4.4|^5.0|^6.0",
        "symfony/http-kernel": "^4.4|^5.0|^6.0"
    },
    "minimum-stability": "dev",
    "require-dev": {
        "symfony/http-kernel": "6.2.*"
    },
    "autoload": {
        "psr-4": {
            "BinaryGeometry\\Bundle\\FruitingBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MyBundleNamespace\\Tests\\": "tests/"
        }
    }
